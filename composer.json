{
    "name": "shikhbas/bas-laravel-sdk",
    "description": "Laravel SDK for BAS Mini Apps Platform",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ShikhBas\\BasLaravelSdk\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
     "extra": {
        "laravel": {
            "providers": [
                "ShikhBas\\BasLaravelSdk\\BasServiceProvider"
            ],
            "aliases": {
                "Bas": "ShikhBas\\BasLaravelSdk\\Facades\\Bas",
                "AuthBas": "ShikhBas\\BasLaravelSdk\\Facades\\AuthBas",
                "PaymentBas": "ShikhBas\\BasLaravelSdk\\Facades\\PaymentBas"
            }
        }
    }
}
